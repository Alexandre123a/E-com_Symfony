{# templates/Acceuil.html.twig #}
{% extends('base.html.twig') %}

{% block javascripts %}
    {{ encore_entry_script_tags('app_acceuil') }}
    {{ parent() }}
{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('css_acceuil') }}
    {{ parent() }}
{% endblock %}

{% block body %}
    <div class="slideshow-container">

        <!-- Full-width images with number and caption text -->
        <div class="mySlides fade">
            <div class="numbertext">1 / 3</div>
            <img src="{{ asset('build/images/paysage1.5e77a739.png') }}" style="width:100%">

        </div>

        <div class="mySlides fade">
            <div class="numbertext">2 / 3</div>
            <img src="{{ asset('build/images/paysage2.f1a0a073.png') }}" style="width:100%">

        </div>

        <div class="mySlides fade">
            <div class="numbertext">3 / 3</div>
            <img src="{{ asset('build/images/paysage3.09f1f610.png') }}" style="width:100%">

        </div>

        <!-- Next and previous buttons -->
        <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
        <a class="next" onclick="plusSlides(1)">&#10095;</a>
    </div>



{% endblock %}

